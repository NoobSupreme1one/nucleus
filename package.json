{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:lambda": "vite build && webpack --config webpack.lambda.js",
    "start": "NODE_ENV=production node dist/index.js",
    "deploy:lambda": "npm run build:lambda && serverless deploy",
    "deploy:apprunner": "aws apprunner create-service --cli-input-json file://apprunner-config.json",
    "check": "tsc",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run client/src",
    "test:integration": "jest --config jest.config.cjs",
    "test:e2e": "playwright test",
    "test:watch": "vitest client/src",
    "test:coverage": "vitest run client/src --coverage && jest --config jest.config.cjs --coverage",
    "test:ci": "npm run test:coverage && npm run test:e2e"
  },
